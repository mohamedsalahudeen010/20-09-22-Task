//lets assign variable a and b, will given as the parameter to the function;
    let a;
    let b;
//assigning an array to push the iterating values;
    let arr=[];
// declare a function range has it's parameters a and b;
     function range(a,b){
// for loop inside the function(range) to iterate the values according to the parameters, which we have given (a to b);
    for (let i=a;i<=b;i++){
// pushing the iterated values to the array arr[];
        arr.push(i);
    }
// returning the function(range);
    return arr;
 }
 
//invocates the function(range) with arguements;
 console.log(range(-4,7));
 });

